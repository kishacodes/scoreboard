{
  "name": "scoreboard",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "wrangler pages dev ./dist",
    "dev:remote": "wrangler pages dev --proxy 4321 --d1=DB -- npm run dev",
    "deploy": "astro build && wrangler pages deploy ./dist",
    "deploy:log": "npm run deploy && npm run log:summary",
    "deploy:log:stdin": "npm run deploy && npm run log:summary:stdin",
    "log:summary": "node scripts/log-summary.mjs docs/last-summary.md",
    "log:summary:stdin": "node scripts/log-summary.mjs --stdin"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^11.0.1",
    "astro": "^4.11.5",
    "bcryptjs": "^3.0.2",
    "hono": "^4.4.12",
    "jose": "^6.0.12"
  },
  "devDependencies": {
    "wrangler": "^4.26.0"
  }
}
